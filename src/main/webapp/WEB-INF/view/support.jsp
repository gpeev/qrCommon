<div class="well well-light-bg">
    <h1>Support</h1>
    <hr>
    <h3>Contact Us</h3>

    <form class="form-vertical" action="/support" method="post">
        <label>Your email</label>
        <input type="text" class="span6" name="contactemail" value="">
        <label>Your message</label>
        <textarea class="span6" name="contactmsg" rows="5"></textarea>
        <input type="hidden" name="posted" value="1">

        <p>
            <button id="submit-button" type="submit" data-loading-text="loading..." autocomplete="off"
                    class="btn btn-primary"><i class="icon-comment icon-white"></i> Send your message
            </button>
        </p>
    </form>
</div>
<hr>